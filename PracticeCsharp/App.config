<?xml version="1.0" encoding="utf-8"?>

<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <log4net>
        <appender name="TranceAppender" type="log4net.Appender.TraceAppender">
            <threshold value="ALL"/>
            <layout type="log4net.Layout.PatternLayout">
                <conversionPattern value="[%date][%level][%property{log4net:HostName}][%property{Message.MessageId}" />
            </layout>
        </appender>
    </log4net>
</configuration>